// IceRealm's toolbox;
(function ($, w, undefined) {
    var tools = {
        random: function (offset, max) {
            return Math.floor(Math.random() * (max - offset)) + offset;
        }
    }

    if (!window.icerealm) window.icerealm = {};
    window.icerealm.tools = tools;
})(jQuery, window, undefined);